{
  "name": "useRouter",
  "version": "0.0.1",
  "main": "scripts/index.js",
  "description": "react use router",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "start": "nodemon scripts/index.js --watch scripts/index.js",
    "build": "webpack --config scripts/webpack.production.js --progress",
    "server": "nodemon scripts/server.js --watch scripts/server.js",
    "test": "jest --colors --coverage",
    "lint": "eslint app/**/*.{js,jsx}",
    "lint:router": "eslint playground/src/router/*.{js,jsx}",
    "lint:use": "eslint playground/src/use/**/*.{js,jsx}",
    "lint:utils": "eslint playground/src/utils/*.js",
    "pkg": "npm run lint:utils && cd playground/publish && NAME=utils webpack --config webpack.build.js",
    "pkg:router": "npm run lint:router && cd playground/publish && NAME=router webpack --config webpack.build.js",
    "pkg:use": "npm run lint:use && cd playground/publish && NAME=use webpack --config webpack.build.js",
    "pkg:utils": "npm run lint:utils && cd playground/publish && NAME=utils webpack --config webpack.build.js",
    "publish": "cd playground/publish/dist/utils && npm version patch && npm publish",
    "publish:router": "cd playground/publish/dist/router && npm version patch && npm publish",
    "publish:use": "cd playground/publish/dist/use && npm version patch && npm publish",
    "publish:utils": "cd playground/publish/dist/utils && npm version patch && npm publish",
    "pub": "npm run pkg && npm run publish",
    "pub:router": "npm run pkg:router && npm run publish:router",
    "pub:use": "npm run pkg:use && npm run publish:use",
    "pub:utils": "npm run pkg:utils && npm run publish:utils"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/huxy"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "http-proxy-middleware": "^1.0.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "morgan": "^1.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.4",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^5.1.2"
  },
  "dependencies": {
    "@huxy/router": "^0.1.10",
    "@huxy/use": "0.0.6",
    "@huxy/utils": "0.0.11",
    "core-js": "^3.6.5",
    "enzyme": "^3.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
